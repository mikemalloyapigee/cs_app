- model_class = TestCase
.page-header
  %h1
    =t '.title', :default => "Test Cases"
%table.table.table-striped
  %thead
    %tr
      %th
        ID 
      %th
        Name 
      %th
        = t '.actions', :default => t("helpers.actions") 
  %tbody
    = render @test_cases
    
= link_to t('.new', :default => t("helpers.links.new")),
            new_test_case_path,
            :class => 'btn btn-primary'
